# keybinding definitions	{{{

#	references:	{{{

##  https://github.com/davidhalter/dotfiles/blob/master/.w3m/keymap
##	https://github.com/tats/w3m/blob/master/doc/README.func

#	}}}

# in_page_navigation	{{{

## page down / up
### half
keymap		C-d		NEXT_HALF_PAGE
keymap		C-u     	PREV_HALF_PAGE
### full
keymap		C-f		NEXT_PAGE
keymap		SPC     	NEXT_PAGE
#keymap		pgFore  	NEXT_PAGE
keymap		PgDn		NEXT_PAGE
keymap		C-b     	PREV_PAGE
#keymap		pgBack  	PREV_PAGE
keymap		PgUp		PREV_PAGE

## move cursor to
### right
#keymap		C-l		MOVE_RIGHT
keymap		RIGHT   	MOVE_RIGHT
#keymap		l       	MOVE_RIGHT	--> next page
### left
keymap		C-h		MOVE_LEFT
keymap		LEFT    	MOVE_LEFT
#keymap		h       	MOVE_LEFT	--> previous page
### down
keymap		j		MOVE_DOWN1
keymap		DOWN    	MOVE_DOWN1
### up
keymap		k		MOVE_UP1
keymap		UP      	MOVE_UP1
### scroll
keymap		C-e		DOWN
keymap		C-y     	UP
### beginning of the line
keymap		0		LINE_BEGIN
### end of the line
keymap		$		LINE_END
### first line
keymap		gg		BEGIN
### specified line number <#>
keymap		<#>gg		GOTO_LINE
### last line
keymap		G		END
### next word
keymap		w		NEXT_WORD
### previous word
keymap		b		PREV_WORD
### first hyperlink
keymap		f		LINK_BEGIN
### next hyperlink
keymap		TAB		NEXT_LINK
keymap		C-n		NEXT_LINK
### previous hyperlink
keymap		M-[Z		PREV_LINK
keymap		C-p		PREV_LINK

## center on
### cursor column
keymap		zh		CENTER_H
### cursor line
keymap		zz		CENTER_V

#	}}}

# hyperlinks and urls	{{{

## list all
keymap		gl		LIST
## list menu
keymap		gF		LIST_MENU
## menu
keymap		gf		MOVE_LIST_MENU
## element menu
keymap		ge		LINK_MENU
## accesskey menu
keymap		ga		ACCESSKEY
## follow (new buffer)
## keymap	ENTER		GOTO_LINK
keymap		C-j     	GOTO_LINK
## follow (new tab)
keymap		F		TAB_LINK
## xdg-open
#keymap		]		EXTERN
#keymap		e       	EXTERN_LINK <urlportal.sh> --> work out urlportal idea
## show url
### current page
keymap		pk		PEEK
### link
keymap		pl		PEEK_LINK
### image
keymap		pi		PEEK_IMG

#	}}}

# images	{{{

## inline image
## view
keymap		i		VIEW_IMAGE
## save							-->	location?
keymap		M-I		SAVE_IMAGE

#	}}}

# information	{{{

## current document
keymap		gi		INFO
## cookies
keymap		gc		COOKIE
## history
keymap		gH		HISTORY
keymap		:h      	HISTORY
## downloads
keymap		gd		DOWNLOAD_LIST
## current line
keymap		C-g		LINE_INFO
## url to hyperlink
keymap		";"		MARK_URL
## word to hyperlink
keymap		,		MARK_WORD

#	}}}

# file / stream operations	{{{

## open
keymap		o		GOTO
## open in new tab
keymap		O		TAB_GOTO
#keymap		gc		TAB_GOTO https://encrypted.google.com
## open relative address
keymap		^o		GOTO_RELATIVE
## open relative address in new tab
keymap		^O		TAB_GOTO_RELATIVE
## open local file
keymap		:e		LOAD
## execute shell command
keymap		@		READ_SHELL
keymap		#		PIPE_SHELL
## pipe current buffer through a shell command
keymap		|		PIPE_BUF

#	}}}

# buffer operations	{{{

## next page
keymap		L		NEXT
## previous page
keymap		H		PREV
## select buffer menu
keymap		gh		SELECT_MENU
## toggle source code
keymap		gv		VIEW
## save source (!to $PWD)
keymap		:w		SAVE
## save rendered document to
keymap		w		SAVE_SCREEN
## reload buffer
keymap		r		RELOAD

#	}}}

# tab operations	{{{

## tab
### close
#keymap		td		CLOSE_TAB	--> conflict with text entry
keymap		ddd		CLOSE_TAB	--> conflict with text entry
### next
#keymap		K		NEXT_TAB
#fix C-] (qutebrowser analogue)
keymap		C-]		NEXT_TAB
keymap		gt		NEXT_TAB
### previous
#keymap		J		PREV_TAB
#fix C-[ (qutebrowser analogue)
keymap		C-\[		PREV_TAB
keymap		gT		PREV_TAB
### menu
keymap		gj		TAB_MENU
### left/right
keymap		C-wL		TAB_RIGHT
keymap		C-wH		TAB_LEFT

#	}}}

# bookmarks			{{{

## view
#keymap				BOOKMARK
## add
#keymap				ADD_BOOKMARK

#	}}}

# search			{{{

## next
keymap		n		SEARCH_NEXT
keymap		/		ISEARCH
## previous
keymap		N		SEARCH_PREV
keymap		?		ISEARCH_BACK

# }}}

# other				{{{

## general menu
keymap		gM		MAIN_MENU
keymap		:H		HELP
keymap		:O		OPTIONS
keymap		:V		VERSION
## mouse on/off
keymap		m		MOUSE_TOGGLE
## reload config file
keymap		cc		REINIT
## commands
keymap		::		COMMAND
keymap		!		EXEC_SHELL

# }}}

# exiting w3m		{{{

keymap		:q		QUIT
keymap		ZZ		QUIT
keymap		ZQ		EXIT
keymap		Q		EXIT

# }}}

# user defined key bindings {{{

## general structure
#keymap		<key>		SHELL			<shell_script_location>
## yank url of current page to clipboard
keymap		yy		EXTERN			'printf %s | wl-copy'
## yank url of active link to clipboard
#keymap		yy		EXTERN_LINK		'printf %s | wl-copy'
## open external image viewer
#keymap		xi		EXTERN_LINK		'w3m -o display_image=1 -o imgdisplay=w3mimgdisplay'
keymap		xf		EXTERN_LINK		'feh -. -x -B black -g 900x600-15+60 %s &'
## open external video player
keymap		xF		EXTERN_LINK		'mpv --loop --quiet --ontop --no-border --force-window --autofit=900x600 --geometry=-15+60'
## add torrent or magnetlink
keymap		xt		EXTERN_LINK		'transmission-remote --add'

# }}}

# scripts & cgi-bin	{{{

# source:
# https://github.com/felipesaa/A-vim-like-firefox-like-configuration-for-w3m

## paste and go
keymap		px		GOTO			/usr/lib/w3m/cgi-bin/goto_clipboard.cgi
keymap		PX		TAB_GOTO		/usr/lib/w3m/cgi-bin/goto_clipboard.cgi
keymap		pp		GOTO			/usr/lib/w3m/cgi-bin/goto_tmux_clipboard.cgi
keymap		PP		TAB_GOTO		/usr/lib/w3m/cgi-bin/goto_tmux_clipboard.cgi
## retore closed tabs
keymap		u		TAB_GOTO		/usr/lib/w3m/cgi-bin/restore_tab.cgi
keymap		C-T		TAB_GOTO		/usr/lib/w3m/cgi-bin/restore_tab.cgi
## close tabs and save url link to file
#keymap		d		COMMAND			"EXTERN 'echo %s >> ~/.config/w3m/restore_tab' ; CLOSE_TAB"
#keymap		C-w		COMMAND			"EXTERN 'echo %s >> ~/.config/w3m/restore_tab' ; CLOSE_TAB"
#open new tab
#keymap		C-t		TAB_GOTO		https://duckduckgo.com/lite/
#keymap		t		TAB_GOTO		https://duckduckgo.com/lite/
#keymap		C-t		COMMAND			"TAB_GOTO https://duckduckgo.com/lite/; NEXT_LINK; GOTO_LINK"
#keymap		t		COMMAND			"TAB_GOTO https://duckduckgo.com/lite/; NEXT_LINK; GOTO_LINK"

# }}}

# other {{{
#
# open bookmarks (surfraw) using fzf
keymap		xb		COMMAND			"SHELL ~/.w3m/cgi-bin/fzf_bookmarks.cgi ; GOTO /usr/lib/w3m/cgi-bin/goto_clipboard_primary.cgi"
keymap		XB      	COMMAND			"SHELL ~/.w3m/cgi-bin/fzf_bookmarks.cgi ; TAB_GOTO /usr/lib/w3m/cgi-bin/goto_clipboard_primary.cgi"
