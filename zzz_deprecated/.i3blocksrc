separator=false
separator_block_width=10

[interface]
interval=2
separator=false
separator_block_width=10

[ipin]
interval=2
separator=false
separator_block_width=8

[macin]
interval=2
separator=false
separator_block_width=10

[vrx]
interval=2
separator=false
separator_block_width=10
color=#BBBBBB

[vtx]
interval=2
separator=false
separator_block_width=10
